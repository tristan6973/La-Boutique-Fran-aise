{% extends 'base.html.twig' %}

{% block content %}
    <h1>Inscription</h1>


    {{ form_start(form) }}
    <div class="row">
        <div class="col-md-6">{{ form_row(form.firstname) }}</div>
        <div class="col-md-6">{{ form_row(form.lastname) }}</div>
        <div class="col-md-12">{{ form_row(form.email) }}</div>
        <div class="col-md-12">{{ form_row(form.password) }}</div>

    </div>
    {{ form_end(form) }}
{% endblock %}